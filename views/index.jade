extends layout

block head
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js')
    script(src='/js/gn.js')

block content
    h1= title
    
    h2#gn-q
    
    form#gn-form
        input#gn-userinput(type="text", spellcheck="false")
        input#gn-submit(type="submit")

    footer
        p#gn-sesh-q
        p#gn-sesh-pts
    
    p#gn-feedback
    
    table#gn-report